{
  "name": "arasjs-types",
  "version": "0.0.5",
  "description": "TypeScript types for Aras Innovator Client-Side API",
  "main": "",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "default": "./index.d.ts"
      }
